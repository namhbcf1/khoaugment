{
  "name": "khoaugment-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173",
    "build": "vite build",
    "build:with-types": "tsc && vite build",
    "build:cloudflare": "node cloudflare-build.cjs",
    "deploy:cloudflare": "npm run build:cloudflare && wrangler pages deploy dist --project-name khoaugment",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,css,md}",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.0",
    "@ant-design/plots": "^2.6.0",
    "antd": "^5.12.0",
    "axios": "^1.6.0",
    "chart.js": "^4.5.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.0",
    "i18next": "^23.7.0",
    "jwt-decode": "^4.0.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.0",
    "react-i18next": "^13.5.0",
    "react-query": "^3.39.0",
    "react-router-dom": "^6.8.0",
    "recharts": "^2.8.0",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240514.0",
    "@playwright/test": "^1.40.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "prettier": "^3.0.0",
    "terser": "^5.43.1",
    "typescript": "^5.0.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "wrangler": "^3.30.1"
  }
}
