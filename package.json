    "deploy": "sh scripts/deploy-all.sh"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "concurrently": "^8.2.1",
    "marked": "^9.0.3",
    "open": "^9.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/username/khoaugment-pos"
  },
  "keywords": [
    "pos",
    "cloudflare",
    "d1",
    "r2",
    "hono",
    "react",
    "vite"
  ],
  "author": "",
