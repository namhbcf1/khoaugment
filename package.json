{
  "name": "khoaugment-pos",
  "version": "1.0.0",
  "type": "module",
  "description": "KhoAugment POS System - Cloudflare Optimized",
  "scripts": {
    "test": "playwright test",
    "test:gpu": "node scripts/gpu-test-runner.js",
    "test:report": "node tests/generate-comprehensive-report.js",
    "view:report": "node scripts/report-viewer.js",
    "start": "cd frontend && npm start",
    "dev": "concurrently \"cd backend && npm run dev\" \"cd frontend && npm run dev\"",
